files <- sprintf("%03d.csv",id)
ret <- NULL
for(file in files) {
content <- read.csv(file)
our <- content[pollutant]
bad <- is.na(our)
need <- our[!bad]
ret[length(ret) + 1] <- mean(need)
}
ret
}
